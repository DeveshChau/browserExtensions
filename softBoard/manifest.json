{
    "manifest_version": 2,
    "name": "Soft Board",
    "version": "0.1",
    "description": "Pin user links just like note on soft borad.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {},
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "options_page": "options.html"
}